var gradeForScore = function(score){
	
	if (score >= 90){
		return "A"
	}
	
	if (score < 90 && score >= 80){
		return "B"
	} 
	
	if (score < 80 && score >= 70){
		return "C"
	}
	
	if (score < 70 && score >= 60){
		return "D"
	}
	
	if (score < 60 && score >= 50){
		return "F"
	}
}
undefined
